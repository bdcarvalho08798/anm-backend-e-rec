
<div class="container">
  <div class="mt-3 card">
  <div class="card-header">Profile</div>
  <div class="card-body">

<% if !@user_profiles.any? %>
<%= link_to "Create Profile", new_user_profile_path, class:"btn btn-sm btn-secondary" %>
<% end %>

<div id="user_profiles">


  <% @user_profiles.each do |user_profile| %>
    <%= render user_profile %>

  <% end %>
</div>
</div>
</div>
</div>


